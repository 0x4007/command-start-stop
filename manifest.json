{
  "name": "Start | Stop",
  "description": "Assign or un-assign yourself from an issue.",
  "ubiquibot:listeners": [ "issue_comment.created" ],
  "commands": {
    "start": {
      "example": "/start",
      "description": "Assign yourself to the issue."
    },
    "stop": {
      "example": "/stop",
      "description": "Unassign yourself from the issue."
    }
  }
}
